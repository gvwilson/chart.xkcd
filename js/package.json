{
  "name": "chart.xkcd",
  "version": "1.1.15",
  "description": "xkcd style chart lib",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "audit": "npm audit",
    "build": "esbuild src/index.js --bundle --platform=browser --format=iife --global-name=chartXkcd --outfile=../src/chart_xkcd/static/chart.xkcd.min.js",
    "start": "esbuild examples/index.js --bundle --servedir=examples --outdir=examples",
    "lint": "./node_modules/.bin/eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gvwilson/chart.xkcd"
  },
  "homepage": "https://github.com/gvwilson/chart.xkcd",
  "keywords": [
    "chart",
    "graph",
    "xkcd",
    "hand-drawn"
  ],
  "author": "timqian",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "esbuild": "^0.25.0",
    "eslint": "^10.0.0",
    "gh-pages": "^6.3.0"
  },
  "dependencies": {
    "d3-axis": "^1.0.12",
    "d3-color": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "dayjs": "^1.8.17"
  }
}
